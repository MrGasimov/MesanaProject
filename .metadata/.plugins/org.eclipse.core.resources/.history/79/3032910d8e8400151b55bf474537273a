package com.corvolution.cm2;

public class SensorConfiguration {
	private String[] sampleRate;

	private String linkId;	
	private String patientData;
	

	
	//return sample rate 
	public String[] getSampleRate() {
		return sampleRate;
	}
	
	//set sample rate for configuration file 
	public void setSampleRate(String[] rate) {
		for(int i=0; i<rate.length:i++ ){
			sampleRate[i] = rate[i];
		}
		
		this.accSampleRate = rate[1];
		this.tempSampleRate= rate[2];
		this.impSampleRate = rate[3];
		
	}
	
	//set linkId for configuration file 
	public void setLinkId(String linkId){
		this.linkId = linkId;
	}
	
	//return linkId 
	public String getLinkId(){
		return linkId;
	}
	
	
	
	
	public void setPatientData(String patientData){
		this.patientData = patientData;
	}

}
