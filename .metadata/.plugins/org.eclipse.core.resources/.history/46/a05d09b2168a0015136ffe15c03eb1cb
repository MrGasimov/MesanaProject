
import java.util.Calendar;
import java.util.Date;

public class Test {

	public static void main(String[] args) {
		 Date date = new Date();
		 Calendar cal = Calendar.getInstance();
		 cal.setTime(date);
		
		 byte y = 35;
		 y += (byte) (cal.get(Calendar.YEAR)-2015);
		 byte m = (byte) cal.get(Calendar.MONTH);
		 byte d = (byte) cal.get(Calendar.DAY_OF_MONTH);
		 System.out.println(y+" "+cal.get(Calendar.MONTH)+" "+ d);
	}
	
	
	
	
}
